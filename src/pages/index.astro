---
const components = await Astro.glob('./../components/*.astro');
---

<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width" />
		<title>Astro</title>
	</head>
	<body>
		<h1>Astro</h1>
    {components.map((comp) => <comp.default />)}
	</body>
</html>
